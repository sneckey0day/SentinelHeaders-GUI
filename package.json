{
  "name": "sentinelheaders-web",
  "version": "1.0.0",
  "description": "Web-based HTTP Security Headers Scanner with Terminal Interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "axios": "^1.5.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^6.10.0",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "security",
    "headers",
    "scanner",
    "web",
    "terminal",
    "mern"
  ],
  "author": "SentinelHeaders Team",
  "license": "MIT"
}